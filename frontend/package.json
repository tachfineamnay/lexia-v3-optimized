{
  "name": "lexiav3-frontend",
  "version": "1.0.0",
  "description": "Frontend for LexiaV3 - AI Assistant for Dyscalculia and Mathematical Learning Difficulties",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "preview:dist": "vite preview --port 4173 --host",
    "analyze": "vite build --mode analyze",
    "clean": "rm -rf dist node_modules/.vite",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy:local": "../deploy-local.sh"
  },
  "keywords": [
    "react",
    "vite",
    "dyscalculia",
    "education",
    "ai-assistant"
  ],
  "author": "Tachfine Amnay",
  "license": "Proprietary",
  "dependencies": {
    "axios": "^1.6.2",
    "docx": "^8.5.0",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^4.0.0",
    "vite": "^6.3.5",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "babel-jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
